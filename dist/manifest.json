{
    "name": "",
    "description": "Build an extension",
    "version": "0.0.1",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "all_frames": true,
          "js": ["content.js"],
          "css": ["content.css"]
        }
      ],
    "action": {
        "default_title": "Replace text",
        "default_icon": {
            "16": "./images/get_started16.png",
            "32": "/images/get_started32.png",
            "48": "/images/get_started48.png",
            "128": "/images/get_started128.png"
        }
    },
    "icons": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
    }
}